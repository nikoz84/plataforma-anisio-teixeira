{
    "name": "laravel/laravel",
    "description": "Plataforma AnÃ­sio Teixeira",
    "keywords": [
        "framework",
        "laravel",
        "vue.js",
        "postgreSQL"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.3.0",
        "aminyazdanpanah/php-ffmpeg-video-streaming": "^1.2",
        "arcanedev/log-viewer": "8.*",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "buglinjo/laravel-webp": "^2.0",
        "calcinai/php-imagick": "^0.1.2",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.4.0",
        "intervention/image": "^2.4",
        "jaybizzle/laravel-crawler-detect": "1.*",
        "jenssegers/date": "4.*",
        "laravel/framework": "8.*",
        "laravel/helpers": "^1.2",
        "laravel/tinker": "^2.0",
        "orbitale/imagemagick-php": "^3.0",
        "spatie/laravel-analytics": "^3.11",
        "tightenco/ziggy": "^1.0",
        "tymon/jwt-auth": "1.0.2"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.13",
        "filp/whoops": "~2.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
