{
    "preset": "psr12",
    "rules": {
        "simplified_null_return": true,
        "braces": false,
        "new_with_braces": {
            "anonymous_class": false,
            "named_class": false
        }
    },
    "exclude": [
        "vendor",
        "app/Console",
        "app/Models",
        "resources/assets",
        "node_modules"
    ]
}